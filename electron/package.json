{
  "name": "dhu-quiz-app",
  "version": "1.0.0",
  "description": "东华红课题库刷题系统",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "set NODE_ENV=development&& electron .",
    "build:win": "set ELECTRON_BUILDER_BINARIES_MIRROR=https://npmmirror.com/mirrors/electron-builder-binaries/&& electron-builder --win --config build/builder-win.yaml",
    "build:mac": "electron-builder --mac --config build/builder-mac.yaml",
    "build:all": "npm run build:win && npm run build:mac",
    "install-python": "node scripts/download-python.js",
    "install-libs": "node scripts/install-python-libs.js",
    "setup-python": "npm run install-python && npm run install-libs",
    "test-python": "node scripts/test-python.js"
  },
  "devDependencies": {
    "electron": "^35.0.0",
    "electron-builder": "^26.0.0",
    "fs-extra": "^11.2.0",
    "adm-zip": "^0.5.14"
  },
  "build": {
    "appId": "com.dhu.quiz",
    "productName": "东华红课题库刷题系统",
    "directories": {
      "output": "dist"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "author": "DHU",
  "license": "MIT"
}
